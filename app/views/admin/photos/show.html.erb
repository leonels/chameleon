      <% unless @photo.image_url.blank? %>
        <div style="background-color:white;border:1px solid black;margin:10px;padding:5px;text-align:center;">
          <%= image_tag @photo.image_url(:thumb).to_s, :class => 'gallery_photo' %>
        </div>
      <% end %>

<p>
  <b>Title:</b>
  <%= @photo.title %>
</p>

<p>
  <b>Caption:</b>
  <%= @photo.caption %>
</p>

<p>
  <b>Gallery:</b>
  <%= @photo.gallery_id %>
</p>
